{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": { "pythonVersion": "3.11" }
    }
  ],
  "routes": [
    { "src": "/roadmap/(.*)", "dest": "/api/roadmap/$1" },
    { "src": "/docs", "dest": "/api/docs" },
    { "src": "/(.*)", "dest": "/api/index.py" }
  ]
}
